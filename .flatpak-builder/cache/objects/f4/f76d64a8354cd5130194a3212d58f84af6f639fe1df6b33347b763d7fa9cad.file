[Application]
name=io.github.Cookiiieee.WSelector
runtime=org.gnome.Platform/x86_64/48
sdk=org.gnome.Sdk/x86_64/48
command=wselector

[Context]
shared=network;
sockets=x11;wayland;fallback-x11;
filesystems=home;xdg-pictures:create;

[Session Bus Policy]
org.kde.plasmashell=talk

[Extension io.github.Cookiiieee.WSelector.Debug]
directory=lib/debug
autodelete=true
no-autodownload=true

[Build]
built-extensions=io.github.Cookiiieee.WSelector.Debug;